    <owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/R8Ae3N5xI9LCDHaLrlUHUOp">
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R71fRNaGgejCUc200GmpJ0X"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R77s1hMVCKD30YK4vNtxmze"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R7qC0YfcQpTTDSGnwgAL1w4"/>
