    <owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/R65e43cd5fed14be0b73eee2">
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R7wzRYrwTet23AkGRoqc0ge"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R9Fd7yCBUJHBmhGKRnTwu3z"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RBmSypqj1L2fBR5oyLkdYDs"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RdwOqVv7gcqrEEVwAwowsH"/>
