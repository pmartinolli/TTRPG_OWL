    <owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/Rdaf151b1e5ad4bf38824d90">
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R0HE86zqI00sz9E6OKQ5o8"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R9bINV2spKsIhby26NHwj4w"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/R9qFOiH6BulgZHFCx3UH00D"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RBIbBYHKCy4eAzdwlMWjnyB"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RCZg4gMK5R5K0IQHlKtVqW9"/>
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RR4o80Gbq7FPANfl9PO4Aj"/>
